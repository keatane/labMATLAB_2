d0 = 2;
d1 = 8;
m = 10*(d0+1)+d1;

% Matrice m x 3 %

A = ones(m,3);
for i = 1:m
	A(i,2) = i/m;
	A(i,3) = (i/m)^2;
end

% Print A %
A;

At = transpose(A);

% Valori singolari %
[U,S,V] = svd(A);
[Ut,St,Vt] = svd(At);


% Si nota che gli unici autovalori non tendenti a zero sono uguali fra AAt e AtA, inoltre %
% i suddetti autovalori corrispondono ai quadrati dei valori singolari di A %
eig(A*At);
eig(At*A);

% Immagine %
ImmA = orth(A);
U;
ImmAt = orth(At);
Ut;

% Nucleo %
% Il nucleo di A viene restituito vuoto (probabilmente perch√® Ut coincide con l'immagine di At)
% Si osserva che le colonne di Vt dalla quarta corrispondono a quelle del nucleo, mentre %
% non sono presenti nel nucleo le prime 3 colonne di Vt %  
KerA = null(A);
V;
KerAt = null(At);
Vt;





